library(edgeR)
count <- read.table("../count/dex_counts_Parker20c.txt", sep = "\t", header = T, skip =1)
counts <- as.matrix(count[,7:18])
colnames(counts) = c("CaiCol0.1", "CaiCol0.2", "CaiCol0.3", "CaiCol0.4", "CaiCol0.5", "CaiCol0.6", "Caifio1.1", "Caifio1.2", "Caifio1.3", "Caifio1.4", "Caifio1.5", "Caifio1.6")
genes <- count[,1:6]

y <- DGEList(counts=counts, genes=genes)
group = factor(c("col0", "col0", "col0", "col0", "col0", "col0", "fio1", "fio1", "fio1", "fio1", "fio1", "fio1"))
y$samples$group <- group

keep <- filterByExpr(y, group=group)
y <- y[keep, , keep.lib.sizes=FALSE]
y <- normLibSizes(y)
design <- model.matrix(~ 0 + group)
design
y
q()
